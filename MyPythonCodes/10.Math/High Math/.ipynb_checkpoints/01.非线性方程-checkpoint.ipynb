     "output_type": "execute_result"
    }
   ],
   "source": [
    "def func_test(x0, *args):\n",
    "    x1, x2 = x0\n",
    "    a, b = args\n",
    "    y1 = x1 + x2 + 2\n",
    "    y2 = a*x1**2 - b*x2 - 8\n",
    "    return y1, y2\n",
    "res = fsolve(func_test, (0, 0), (1, 1))\n",
    "res\n"
   ]
  }
